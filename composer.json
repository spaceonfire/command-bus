  "description": "",
  "keywords": [
    "spaceonfire",
    "command-bus"
  ],
  "homepage": "https://github.com/spaceonfire/command-bus",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://www.onfire.space",
